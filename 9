exp 11 
#!/bin/bash

echo "Exp 11 : Print whether vowel or consonant"
echo "Skill lab"
echo ""

echo "Enter a character: "
read char

if [[ $char =~ [aeiouAEIOU] ]]; then
    echo "vowel"
else
    echo "consonant"
fi

